<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="egovframework.burin.cmmn.dao.NodeDao">

<!-- 전체노선 조회 -->
<select id="findNodesByRouteNm" resultType="egovframework.burin.cmmn.vo.NodeVO">
    SELECT * 
    FROM node 
    WHERE route_nm = #{routeNm}
</select>




</mapper>

