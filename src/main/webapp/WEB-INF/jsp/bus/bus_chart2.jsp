// 	console.log("구 내 버스 정류장 개수: ", countInYongsan);
	
	// 필요한 경우 계산된 개수를 페이지의 특정 요소에 표시
// 	document.getElementById("busStopCount").innerText = "구 내 버스 정류장 개수: " + countInYongsan;
	
	function isPointInPolygon(point, polygon) {
	    var x = point[0], y = point[1];
	    var inside = false;
	
	    for (var i = 0, j = polygon.length - 1; i < polygon.length; j = i++) {
	        var xi = polygon[i][0], yi = polygon[i][1];
	        var xj = polygon[j][0], yj = polygon[j][1];
	
	        var intersect = ((yi > y) !== (yj > y))
	            && (x < (xj - xi) * (y - yi) / (yj - yi) + xi);
	        if (intersect) inside = !inside;
	    }
	
	    return inside;
	}
	
	// 모든 구에 대한 버스 정류장 개수 계산
	var areasCount = areas.map(function(area) {
	    var count = busStops.reduce(function(acc, busStop) {
	        var point = [busStop[0], busStop[1]];
	        if (isPointInPolygon(point, area.path.map(function(latlng) {
	            return [latlng.getLng(), latlng.getLat()];
	        }))) {
	            return acc + 1;
	        } else {
	            return acc;
	        }
	    }, 0);
	    return {name: area.name, count: count};
	});

	// 버스 정류장 개수에 따라 내림차순 정렬
	areasCount.sort(function(a, b) {
